{
  "name": "one",
  "displayName": "One",
  "description": "One for VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vscodeOne"
  ],
  "icon": "icon.png",
  "main": "./src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscodeOne",
          "title": "「ONE · 一个」",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "vscodeOne": [
        {
          "id": "vscodeOne",
          "name": ""
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "vscodeOne.refresh",
          "group": "navigation",
          "when": "view == vscodeOne"
        }
      ],
      "view/item/context": [
        {
          "command": "vscodeOne.see",
          "when": "view == vscodeOne &&  viewItem == see",
          "group": "inline"
        }
      ]
    },
    "commands": [
      {
        "command": "vscodeOne.refresh",
        "title": "刷新",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "vscodeOne.see",
        "title": "查看详情",
        "icon": {
          "light": "resources/light/eye.svg",
          "dark": "resources/dark/eye.svg"
        }
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "eslint": "^5.13.0",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "superagent": "^5.1.0",
    "image-downloader": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yrming/vscode-one"
  },
  "publisher": "YRM"
}
